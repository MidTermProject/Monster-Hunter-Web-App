
<nav class="topnav">
    <a class="active" href="#home">Home</a>
    <a href="#weapons">Sign Up</a>
    <a href="#filter">Filter</a>
    <a href="#about">About</a>
</nav>

<nav class="topnav">
    <ul id="leftfloat">
        <li id="img"><img src="~/Img/homeimg.jpg" width="75" height="35">Monster Hunter</li>
    </ul>
    <ul id="rightfloat">
        @if (ViewData["LoggedInUserName"] != null)
        {
            <li>Hi @ViewData["LoggedInUserName"].ToString()!</li>
            <li id="MyWeapons"><a asp-action="UserFavoriteWeapons" asp-controller="UserModel" asp-route-userName="@ViewData["LoggedInUserName"].ToString()"><img src="~/Img/savedweapons.png">My Saved Weapons</a></li>
        }
        else
        {
            <li><a asp-action="UserSignUp" asp-controller="UserModel" asp-route-id="">Register</a></li>
            <li><a href="#about">About</a></li>
        }
    </ul>
</nav>

